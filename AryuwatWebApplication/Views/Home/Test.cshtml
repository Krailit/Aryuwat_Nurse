@using AryuwatWebApplication.Models
@{
    ViewBag.Title = "Test";
}
<script src="http://code.highcharts.com/highcharts.js"></script>
<script src="http://code.highcharts.com/modules/exporting.js"></script>
<script src="http://code.highcharts.com/themes/grid.js"></script>
<div id="app" style="font-size:30px">
    <div id="content">
        <div id="container_BP" style="width: 1000px; height: 400px; margin: 0 auto"></div>
    </div>
</div>
<script src="~/Scripts/vue.js"></script>
<script src="~/Scripts/axios.js"></script>
<script src="~/Scripts/moment.js"></script>
<script>
    $(function () {
        var chart;
        $(document).ready(function () {

            var container_BP = new Highcharts.Chart({
                chart: {
                    renderTo: 'container_BP',
                    type: 'line'
                },
                title: {
                    text: 'Blood Pressure'
                },
                //subtitle: {
                //    text: 'Measured using Philips BP Monitor'
                //},
                xAxis: {
                    type: 'datetime',
                    dateTimeLabelFormats: { // don't display the dummy year
                        month: '%e. %b',
                        year: '%b'
                    }
                },
                yAxis: [{
                    title: {
                        text: 'Blood Pressure (mmHg)'
                    },
                    lineWidth: 1,
                    minorTickWidth: 1,
                    minorTickLength: 3


                }, {
                    title: {
                        text: 'Heart Rate (b/m)'
                    },
                    opposite: true
                }],
                tooltip: {
                    formatter: function () {
                        var s = '<b>' + Highcharts.dateFormat('%b-%e', this.x) + '</b><br/>';
                        s += 'Blood Pressure: ' + this.points[0].point.y + '/' + this.points[1].point.y + ' mmHg' + '<br/>';
                        return s;
                    },
                    crosshairs: true,
                    shared: true
                },

                plotOptions: {
                    series: {
                        animation: false,
                        marker: {
                            radius: 3
                        },
                        fillopacity: 0.1
                    }
                },

                series: [{
                    name: 'Pulse (Systolic)',
                    color: '#006376',
                    // Systolic BP
                    data: [
                        [Date.UTC(2013, 11, 11, 0, 0, 0), 140],
                        [Date.UTC(2013, 11, 11, 4, 0, 0), 161],
                        [Date.UTC(2013, 11, 11, 8, 0, 0), 120],
                        [Date.UTC(2013, 11, 11, 12, 0, 0), 111],
                        [Date.UTC(2013, 11, 12, 0, 0, 0), 140],
                        [Date.UTC(2013, 11, 12, 4, 0, 0), 161],
                        [Date.UTC(2013, 11, 12, 8, 0, 0), 120],
                        [Date.UTC(2013, 11, 12, 12, 0, 0), 111],
                    ]
                }, {
                        name: 'Pulse (Diastolic)',
                    data: [
                        [Date.UTC(2013, 11, 11, 0, 0, 0), 85],
                        [Date.UTC(2013, 11, 11, 4, 0, 0), 79],
                        [Date.UTC(2013, 11, 11, 8, 0, 0), 84],
                        [Date.UTC(2013, 11, 11, 12, 0, 0), 79],
                        [Date.UTC(2013, 11, 12, 0, 0, 0), 85],
                        [Date.UTC(2013, 11, 12, 4, 0, 0), 79],
                        [Date.UTC(2013, 11, 12, 8, 0, 0), 84],
                        [Date.UTC(2013, 11, 12, 12, 0, 0), 79]
                    ]
                }]
            });
        });
    });
    Vue.prototype.$http = axios;
    const app = new Vue({
        el: "#app",
        data: {
        },
        mounted: function () {
        },
        watch: {
        },
        methods: {
        }
    });
</script>
